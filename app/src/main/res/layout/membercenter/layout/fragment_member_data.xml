<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".membercenter.controller.MemberDataFragment">

    <data>
        <variable
            name="viewModel"
            type="com.example.android_traffic.membercenter.viewmodel.MemberDataViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">



        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_Avatar"
            android:layout_width="0dp"
            android:layout_height="62dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="20dp"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/tv_MemberData_Avatar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_MemberData_Avatar"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/img_MemberData_Avatar"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginEnd="28dp"
                android:src="@drawable/ivy"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="19dp"
                android:layout_height="29dp"
                android:src="@drawable/membercenter_txt_edit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="367dp"
                android:layout_height="0dp"
                android:layout_marginEnd="4dp"
                android:src="@drawable/membercenter_txt_bottomline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_Ncikname"
            android:layout_width="0dp"
            android:layout_height="62dp"

            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="@+id/cl_Avatar"
            app:layout_constraintStart_toStartOf="@+id/cl_Avatar"
            app:layout_constraintTop_toBottomOf="@+id/cl_Avatar">

            <TextView
                android:id="@+id/tv_MemberData_Nickname_Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_MemberCenterData_Nickname"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_MemberData_Nickname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="28dp"
                android:text="豹豹 (寫死 待改)"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="19dp"
                android:layout_height="29dp"
                android:src="@drawable/membercenter_txt_edit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="367dp"
                android:layout_height="0dp"
                android:layout_marginEnd="4dp"
                android:src="@drawable/membercenter_txt_bottomline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_Email"
            android:layout_width="0dp"
            android:layout_height="62dp"

            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="@+id/cl_Ncikname"
            app:layout_constraintStart_toStartOf="@+id/cl_Ncikname"
            app:layout_constraintTop_toBottomOf="@+id/cl_Ncikname">

            <TextView
                android:id="@+id/tv_MemberData_Email_Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_MemberCenterData_Email"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_MemberData_Email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="28dp"
                android:text="seal@seal.com (寫死 待改)"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="19dp"
                android:layout_height="29dp"
                android:src="@drawable/membercenter_txt_edit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="367dp"
                android:layout_height="0dp"
                android:layout_marginEnd="4dp"
                android:src="@drawable/membercenter_txt_bottomline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_BirthDay"
            android:layout_width="0dp"
            android:layout_height="62dp"

            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="@+id/cl_Email"
            app:layout_constraintStart_toStartOf="@+id/cl_Email"
            app:layout_constraintTop_toBottomOf="@+id/cl_Email">

            <TextView
                android:id="@+id/tv_MemberData_BirthDay_Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_RelatedPersonData_BirthdayTitle"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_MemberData_BirthDay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="28dp"
                android:text="999-99-99 (寫死 待改)"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="19dp"
                android:layout_height="29dp"
                android:src="@drawable/membercenter_txt_edit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="367dp"
                android:layout_height="0dp"
                android:layout_marginEnd="4dp"
                android:src="@drawable/membercenter_txt_bottomline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_Address"
            android:layout_width="0dp"
            android:layout_height="62dp"

            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="@+id/cl_BirthDay"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/cl_BirthDay"
            app:layout_constraintTop_toBottomOf="@+id/cl_BirthDay">

            <TextView
                android:id="@+id/tv_MemberData_Address_Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_MemberCenterData_Address"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_MemberData_Address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="28dp"
                android:text="海豹國喵喵市呱呱路0號999樓 (寫死 待改)"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <ImageView
                android:layout_width="19dp"
                android:layout_height="29dp"
                android:src="@drawable/membercenter_txt_edit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="367dp"
                android:layout_height="0dp"
                android:layout_marginEnd="4dp"
                android:src="@drawable/membercenter_txt_bottomline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_VehicleData"
            android:layout_width="0dp"
            android:layout_height="62dp"

            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="@+id/cl_Address"
            app:layout_constraintStart_toStartOf="@+id/cl_Address"
            app:layout_constraintTop_toBottomOf="@+id/cl_Address">

            <TextView
                android:id="@+id/tv_MemberData_VehicleData_Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_MemberCenter_VehicleData"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <ImageView
                android:layout_width="19dp"
                android:layout_height="29dp"
                android:src="@drawable/membercenter_txt_edit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="367dp"
                android:layout_height="0dp"
                android:layout_marginEnd="4dp"
                android:src="@drawable/membercenter_txt_bottomline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_EditPassword"
            android:layout_width="0dp"
            android:layout_height="62dp"

            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="@+id/cl_VehicleData"
            app:layout_constraintStart_toStartOf="@+id/cl_VehicleData"
            app:layout_constraintTop_toBottomOf="@+id/cl_VehicleData">

            <TextView
                android:id="@+id/tv_MemberData_EditPassword_Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_MemberCenterData_EditPassword"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <ImageView
                android:layout_width="19dp"
                android:layout_height="29dp"
                android:src="@drawable/membercenter_txt_edit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="367dp"
                android:layout_height="0dp"
                android:layout_marginEnd="4dp"
                android:src="@drawable/membercenter_txt_bottomline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>