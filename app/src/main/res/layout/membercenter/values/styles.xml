<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--AlertDialog消除白色背景 變透明-->
    <style name="AlertDialogCustomStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <!-- 圆角图片Style -->
    <style name="RoundStyle">
        <item name="cornerSizeTopLeft">8dp</item>
        <item name="cornerSizeTopRight">8dp</item>
        <item name="cornerSizeBottomRight">8dp</item>
        <item name="cornerSizeBottomLeft">8dp</item>
    </style>

    <!-- 圆形图片 -->
    <style name="circleImageStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

</resources>