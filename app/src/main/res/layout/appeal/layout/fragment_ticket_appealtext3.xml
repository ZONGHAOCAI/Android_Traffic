<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ticketappealtext.controller.TicketAppealtext3Fragment">

    <data>

        <variable
            name="viewModel"
            type="com.example.android_traffic.ticketappealtext.viewmodel.TicketAppealtext3ViewModel" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textAlignment="center">

        <TextView
            android:id="@+id/ttterrocon"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_marginBottom="60dp"
            android:textAlignment="center"
            android:textColor="#FF0000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tttconfirm2" />

        <CheckBox
            android:id="@+id/tttconfirm2"
            android:layout_width="119dp"
            android:layout_height="46dp"
            android:layout_marginStart="30dp"
            android:layout_marginBottom="90dp"
            android:text="確認"
            android:textAlignment="gravity"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/goback2"
            android:layout_width="137dp"
            android:layout_height="46dp"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="10dp"
            android:layout_weight="1"
            android:backgroundTint="#EC8787"
            android:paddingLeft="16dp"
            android:text="@string/goback"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/gonext2"
            android:layout_width="137dp"
            android:layout_height="46dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="10dp"

            android:backgroundTint="#EC8787"
            android:text="送出"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/ttt"
            android:layout_width="228dp"
            android:layout_height="51dp"
            android:layout_marginTop="2dp"
            android:hint="@string/tttnew"
            android:textAlignment="center"
            android:textColor="#040303"
            android:textColorHint="#040303"
            android:textColorLink="#190606"
            android:textSize="34sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView14" />

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="399dp"
            android:layout_height="340dp"
            app:layout_constraintBottom_toTopOf="@+id/tttconfirm2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ttt">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttID"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView22" />

                <TextView
                    android:id="@+id/tvid2"
                    android:layout_width="205dp"
                    android:layout_height="41dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvusername2" />

                <TextView
                    android:id="@+id/tvusername2"
                    android:layout_width="197dp"
                    android:layout_height="49dp"
                    android:layout_marginTop="14dp"
                    android:layout_marginEnd="12dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.666"
                    app:layout_constraintStart_toEndOf="@+id/textView22"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="142dp"
                    android:layout_height="29dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:hint="@string/tttusername"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tttphone"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttPhone"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#0B0A0A"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView23" />

                <TextView
                    android:id="@+id/fafafafa"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttEmail"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttphone" />

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttaddress"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fafafafa" />

                <TextView
                    android:id="@+id/textView27"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttproject"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView26" />

                <TextView
                    android:id="@+id/textView28"
                    android:layout_width="171dp"
                    android:layout_height="36dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttoffender"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
