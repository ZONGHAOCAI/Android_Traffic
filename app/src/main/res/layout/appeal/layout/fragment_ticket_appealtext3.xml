                    android:hint="@string/tttoddd"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView29" />

                <TextView
                    android:id="@+id/tttcontent"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttcontent"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttoddd" />

                <TextView
                    android:id="@+id/textView35"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="80dp"
                    android:layout_marginBottom="40dp"
                    android:hint="@string/tttfile"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttcontent" />

                <TextView
                    android:id="@+id/tvphone2"
                    android:layout_width="202dp"
                    android:layout_height="42dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginEnd="25dp"
                    android:ems="10"
                    android:inputType="number"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvid2" />

                <TextView
