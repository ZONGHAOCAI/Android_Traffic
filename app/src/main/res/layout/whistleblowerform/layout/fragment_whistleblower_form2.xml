<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".whistleblowerform.WhistleblowerForm2Fragment" >

    <Button
        android:id="@+id/btnNextWhistleblower2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:text="@string/txt_WhistleblowerForm_Next"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toStartOf="@+id/btnUploadDocWhistleblower2"
        app:layout_constraintTop_toBottomOf="@+id/btnUploadDocWhistleblower2" />

    <Button
        android:id="@+id/btnUploadDocWhistleblower2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="232dp"
        android:text="@string/txt_WhistleblowerForm_UploadFile"
        app:layout_constraintEnd_toEndOf="@+id/tvUploadDocWhistleblower2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tvUploadDocWhistleblower2"
        app:layout_constraintTop_toBottomOf="@+id/tvUploadDocWhistleblower2" />

    <Button
        android:id="@+id/btnPreviousWhistleblower2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:text="@string/txt_WhistleblowerForm_Previous"
        app:layout_constraintEnd_toEndOf="@+id/btnUploadDocWhistleblower2"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnUploadDocWhistleblower2" />

    <TextView
        android:id="@+id/tvUploadDocWhistleblower2"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_marginTop="72dp"
        android:text="@string/txt_WhistleblowerForm_UploadDoc"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvContentWhistleblower2"
        android:layout_width="230dp"
        android:layout_height="171dp"
        android:layout_marginTop="148dp"
        android:text="@string/txt_WhistleblowerForm_Content"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>