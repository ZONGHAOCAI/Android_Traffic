<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ticketappealtext.controller.TicketAppealtextFragment">

    <data>

        <variable
            name="viewModel"
            type="com.example.android_traffic.ticketappealtext.viewmodel.TicketAppealtextViewModel" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textAlignment="center">

        <TextView
            android:id="@+id/ttterro2"
            android:layout_width="410dp"
            android:layout_height="28dp"
            android:layout_marginBottom="10dp"
            android:textAlignment="center"
            android:textColor="#EA0404"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/tttup"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/goback"
            android:layout_width="137dp"
            android:layout_height="46dp"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="10dp"
            android:layout_weight="1"
            android:backgroundTint="#F47D7D"
            android:paddingLeft="16dp"
            android:text="@string/goback"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/tttup"
            android:layout_width="181dp"
            android:layout_height="51dp"
            android:layout_marginBottom="80dp"
            android:backgroundTint="#EC8787"
            android:text="上傳檔案"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/gonext"
            android:layout_width="137dp"
            android:layout_height="46dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="10dp"
            android:layout_weight="1"
            android:backgroundTint="#EC8787"
            android:text="@string/gonext"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
        android:id="@+id/imageView12"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:src="@drawable/ticketnumber2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ttt"
        android:layout_width="209dp"
        android:layout_height="51dp"
        android:hint="@string/ttt"
        android:textAlignment="center"
        android:textColor="#040303"
        android:textColorHint="#040303"
        android:textColorLink="#190606"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="406dp"
            android:layout_height="305dp"
            app:layout_constraintBottom_toTopOf="@+id/ttterro2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ttt">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttID"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView22" />

                <EditText
                    android:id="@+id/tvid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="10dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvusername" />

                <EditText
                    android:id="@+id/tvusername"
                    android:layout_width="197dp"
                    android:layout_height="49dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="12dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/textView22"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="142dp"
                    android:layout_height="29dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:hint="@string/tttusername"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tttphone"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttPhone"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#0B0A0A"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView23" />

                <TextView
                    android:id="@+id/fafafafa"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttEmail"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttphone" />

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttaddress"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fafafafa" />

                <TextView
                    android:id="@+id/textView27"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttproject"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView26" />

                <TextView
                    android:id="@+id/textView28"
                    android:layout_width="171dp"
                    android:layout_height="36dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttoffender"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView27" />

                <TextView
                    android:id="@+id/textView29"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttlicense"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView28" />

                <TextView
                    android:id="@+id/tttoddd"
                    android:layout_width="149dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttoddd"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView29" />

                <TextView
                    android:id="@+id/tttcontent"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:hint="@string/tttcontent"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttoddd" />

                <TextView
                    android:id="@+id/textView35"
                    android:layout_width="138dp"
                    android:layout_height="37dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="80dp"
                    android:layout_marginBottom="40dp"
                    android:hint="@string/tttfile"
                    android:textAlignment="center"
                    android:textColorHint="#000000"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttcontent" />

                <EditText
                    android:id="@+id/tvphone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="10dp"
                    android:ems="10"
                    android:inputType="number"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvid" />

                <EditText
                    android:id="@+id/tvemail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="14dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvphone" />

                <EditText
                    android:id="@+id/tttaddress"
                    android:layout_width="225dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="10dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvemail" />

                <Spinner
                    android:id="@+id/tttmenu"
                    android:layout_width="226dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="20dp"
                    android:entries="@array/txtArray"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttaddress" />

                <EditText
                    android:id="@+id/tttoddname"
                    android:layout_width="179dp"
                    android:layout_height="43dp"
                    android:layout_marginTop="30dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttmenu" />

                <EditText
                    android:id="@+id/tttcarnamber"
                    android:layout_width="225dp"
                    android:layout_height="51dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="10dp"
                    android:ems="10"
                    android:inputType="numberSigned"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttoddname" />

                <EditText
                    android:id="@+id/tttupnumber"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="10dp"
                    android:ems="10"
                    android:inputType="number"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttcarnamber" />

                <EditText
                    android:id="@+id/tttcon"
                    android:layout_width="218dp"
                    android:layout_height="44dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginEnd="12dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="@={viewModel.con}"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttupnumber" />

                <EditText
                    android:id="@+id/tttcontwo"
                    android:layout_width="385dp"
                    android:layout_height="51dp"
                    android:layout_marginTop="10dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="@={viewModel.contwo}"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tttcon" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
    </layout>